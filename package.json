    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "helmet": "^3.4.0",
    "http": "0.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongodb": "^2.2.22",
    "mongoose": "^4.8.2",
    "morgan": "^1.8.1",
