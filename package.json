{
  "name": "ng-component-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "server/bin/www.js",
  "scripts": {
    "postinstall": "tsc",
    "analyze": "ANALYZE=1 npm run build",
    "build:client": "webpack",
    "build:sass": "node-sass ./client/styles/ -o ./client/styles/",
    "build": "tsc && npm run build:client && npm run build:sass",
    "build:prod": "PROD_ENV=1 npm run build",
    "dev": "tsc && concurrently \"webpack -w\" \"node-sass client/styles/ -o client/styles/ -w\" \"nodemon\"",
    "dev:prod": "PROD_ENV=1 npm run dev",
    "serve": "nodemon",
    "start": "tsc && npm run watch"
  },
