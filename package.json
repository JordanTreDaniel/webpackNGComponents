{
  "name": "ng-component-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "server/bin/www",
  "scripts": {
    "postinstall": "npm run build",
    "analyze": "ANALYZE=1 npm run build",
    "analyze:prod": "ANALYZE=1 PROD_ENV=1 npm run build",
    "build:server": "tsc -p server",
    "build:client": "tsc -p client && node_modules/.bin/webpack",
    "build:sass": "node-sass ./client/styles/ -o ./client/styles/",
    "build": "npm run build:client && npm run build:server && npm run build:sass",
    "build:prod": "PROD_ENV=1 npm run build",
    "watch": "concurrently \"node_modules/.bin/webpack-dev-server\" \"node-sass client/styles/ -o client/styles/ -w\"",
    "watch:prod": "PROD_ENV=1 npm run watch",
    "start": "npm run watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "^3.3.0",
    "html-webpack-plugin": "^2.28.0",
    "ng-annotate-webpack-plugin": "^0.2.1-pre",
    "node-sass": "^4.5.0",
    "noop-loader": "^1.0.0",
    "open-browser-webpack-express-plugin": "0.0.3",
    "open-browser-webpack-plugin": "0.0.3",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0"
  },
  "dependencies": {
    "@types/angular": "^1.6.5",
    "@types/angular-resource": "^1.5.8",
    "@types/angular-sanitize": "^1.3.4",
    "@types/body-parser": "0.0.33",
    "@types/connect-mongo": "0.0.32",
    "@types/debug": "0.0.29",
    "@types/ejs": "^2.3.33",
    "@types/express": "^4.0.35",
    "@types/express-session": "0.0.32",
    "@types/helmet": "0.0.33",
    "@types/lodash": "^4.14.52",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^2.1.40",
    "@types/mongoose": "^4.7.5",
    "@types/morgan": "^1.7.32",
    "@types/ngstorage": "^0.3.34",
    "@types/passport": "^0.3.3",
    "@types/passport-local": "^1.0.29",
    "@types/validator": "^6.2.0",
    "angular-animate": "^1.6.2",
    "angular-resource": "^1.6.2",
    "angular-sanitize": "^1.6.2",
    "angular-ui-router": "^0.4.2",
    "body-parser": "^1.16.1",
